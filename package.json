{
    "name": "dp-track-time",
    "version": "1.0.0",
    "description": "Utility to help players count their track record time",
    "main": "src/index.ts",
    "type": "module",
    "engines": {
        "node": ">=16.6.0",
        "npm": ">=7.0.0"
    },
    "scripts": {
        "clean": "rimraf build",
        "dev": "ts-node-esm src/index.ts",
        "build": "npm run clean && tsc -p tsconfig.release.json",
        "build:watch": "tsc -w -p tsconfig.release.json",
        "lint": "eslint src/**/*.ts",
        "format": "eslint src/**/*.ts --fix"
    },
    "author": "arcetros",
    "license": "MIT",
    "devDependencies": {
        "@types/figlet": "^1.5.5",
        "@types/fs-extra": "^11.0.1",
        "@types/inquirer": "^9.0.3",
        "@types/node": "^18.15.3",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "eslint": "^8.36.0",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "plugin:@typescript-eslint/recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 2018,
            "sourceType": "module"
        },
        "rules": {}
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "printWidth": 120,
        "proseWrap": "never"
    },
    "dependencies": {
        "@types/ramda": "^0.28.23",
        "chalk": "^5.2.0",
        "child-process": "^1.0.2",
        "execa": "npm:@esm2cjs/execa@^6.1.1-cjs.1",
        "figlet": "^1.5.2",
        "fs-extra": "^11.1.0",
        "image-js": "^0.35.3",
        "inquirer": "^9.1.4",
        "ora": "^6.1.2",
        "ramda": "^0.28.0",
        "rimraf": "^4.4.0",
        "xml2js": "^0.4.23"
    }
}
