{
    "name": "dp-track-time",
    "version": "1.0.0",
    "description": "Utility to help players count their track record time",
    "main": "src/index.ts",
    "engines": {
        "node": ">=16.6.0",
        "npm": ">=7.0.0"
    },
    "scripts": {
        "dev": "ts-node .",
        "lint": "eslint src/**/*.ts",
        "format": "eslint src/**/*.ts --fix"
    },
    "author": "arcetros",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18.14.2",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "eslint": "^8.35.0",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "plugin:@typescript-eslint/recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 2018,
            "sourceType": "module"
        },
        "rules": {}
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "printWidth": 120
    }
}
